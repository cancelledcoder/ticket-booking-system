# Production Database URL (you'll get this from Neon)
DATABASE_URL="postgresql://[user]:[password]@[endpoint]/[dbname]?sslmode=require"
DIRECT_URL="postgresql://[user]:[password]@[endpoint]/[dbname]?sslmode=require"

# Production JWT Secret (use a strong random string)
JWT_SECRET="your-production-jwt-secret"

# Node Environment
NODE_ENV="production" 